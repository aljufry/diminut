<% @page_title = t(:edit_subnet) %>

<div class="pageTitle">
  <%= t(:edit_subnet) %>
</div>

<div class="form">
  <%= form_for(:subnet, :url => {:action => 'update', :id => @subnet.id}) do |f| %>
  <% if @subnet.errors.any? %>
  
  <div class="error">
    <ul>
      <% @subnet.errors.full_messages.each do |message| %>
      <li>
        <%= message %>
      </li>
      <% end %>
    </ul>
  </div>
  <% end %>
  <%= render(:partial => "form", :locals => {:f => f}) %>
  <% end %>
</div>
